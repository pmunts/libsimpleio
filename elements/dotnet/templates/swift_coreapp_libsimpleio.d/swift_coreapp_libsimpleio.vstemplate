<VSTemplate Version="3.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" xmlns:Elements="http://schemas.remobjects.com/elements/vstemplate">
  <TemplateData>
    <Name>RemObjects Elements Swift console application for .Net Core using libsimpleio</Name>
    <Description>A project for building a Swift console application for .Net Core using libsimpleio.</Description>
    <Icon>swift_coreapp_libsimpleio.ico</Icon>
    <ProjectType>Oxygene</ProjectType>
    <Elements:Type>Oxygene.Echoes</Elements:Type>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>swift_coreapp_libsimpleio</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LanguageTag>Swift</LanguageTag>
    <LanguageTag>RemObjects Swift</LanguageTag>
    <PlatformTag>.Net Core</PlatformTag>
    <PlatformTag>Linux</PlatformTag>
    <ProjectTypeTag>Console</ProjectTypeTag>
    <ProjectTypeTag>libsimpleio</ProjectTypeTag>
    <ProjectTypeTag>Munts Technologies Linux Simple I/O Library client</ProjectTypeTag>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="swift_coreapp_libsimpleio.elements" File="swift_coreapp_libsimpleio.elements" ReplaceParameters="true">
      <Folder Name="Properties" TargetFolderName="Properties">
        <ProjectItem ReplaceParameters="false" TargetFileName="App.ico">App.ico</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="AssemblyInfo.swift">AssemblyInfo.swift</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Resources.resx">Resources.resx</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Resources.Designer.swift">Resources.Designer.swift</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Settings.settings">Settings.settings</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Settings.Designer.swift">Settings.Designer.swift</ProjectItem>
      </Folder>
      <ProjectItem ReplaceParameters="false" TargetFileName="Makefile">Makefile</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="Program.swift">Program.swift</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>
