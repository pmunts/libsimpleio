with "config/test_signal_level_tx_config.gpr";
project Test_Signal_Level_Tx is

   for Source_Dirs use ("src/", "config/");
   for Object_Dir use "obj/" & Test_Signal_Level_Tx_Config.Build_Profile;
   for Create_Missing_Dirs use "True";
   for Exec_Dir use "bin";
   for Main use ("test_signal_level_tx.adb");

   package Compiler is
      for Default_Switches ("Ada") use Test_Signal_Level_Tx_Config.Ada_Compiler_Switches & ("-gnaty-g");
   end Compiler;

   package Binder is
      for Switches ("Ada") use ("-Es"); --  Symbolic traceback
   end Binder;

   package Install is
      for Artifacts (".") use ("share");
   end Install;

end Test_Signal_Level_Tx;
