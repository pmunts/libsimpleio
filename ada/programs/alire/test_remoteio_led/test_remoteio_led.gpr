with "config/test_remoteio_led_config.gpr";
project Test_RemoteIO_LED is

   for Source_Dirs use ("src/", "config/");
   for Object_Dir use "obj/" & Test_RemoteIO_LED_Config.Build_Profile;
   for Create_Missing_Dirs use "True";
   for Exec_Dir use "bin";
   for Main use ("test_remoteio_led.adb");

   package Compiler is
      for Default_Switches ("Ada") use Test_RemoteIO_LED_Config.Ada_Compiler_Switches & ("-gnaty-g");
   end Compiler;

   package Binder is
      for Switches ("Ada") use ("-Es"); --  Symbolic traceback
   end Binder;

   package Install is
      for Artifacts (".") use ("share");
   end Install;

end Test_RemoteIO_LED;
